<?xml version="1.0" encoding="UTF-8"?>
<window>
	<!-- startup -->
	<onload condition="System.HasAddon(script.skin.helper.service) + System.GetBool(input.enablemouse)">RunScript(script.skin.helper.service,action=togglekodisetting,setting=input.enablemouse)</onload>
	<onload condition="System.HasAddon(script.skin.helper.service) + System.GetBool(filelists.showparentdiritems)">RunScript(script.skin.helper.service,action=togglekodisetting,setting=filelists.showparentdiritems)</onload>
	<onload condition="System.HasAddon(script.skin.helper.service) + System.GetBool(musiclibrary.showallitems)">RunScript(script.skin.helper.service,action=togglekodisetting,setting=musiclibrary.showallitems)</onload>
	<onload>ReplaceWindow($INFO[System.StartupWindow])</onload>
</window>